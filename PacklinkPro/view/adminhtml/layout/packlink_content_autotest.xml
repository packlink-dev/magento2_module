<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title></title>
        <css src="Packlink_PacklinkPro::css/packlink.css"/>
        <css src="Packlink_PacklinkPro::css/packlink-auto-test.css"/>
        <link src="Packlink_PacklinkPro::packlink/js/AjaxService.js"/>
        <link src="Packlink_PacklinkPro::packlink/js/TemplateService.js"/>
        <link src="Packlink_PacklinkPro::packlink/js/UtilityService.js"/>
        <link src="Packlink_PacklinkPro::packlink/js/AutoTestController.js"/>
    </head>
    <body>
        <referenceContainer name="page.messages">
            <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Packlink\PacklinkPro\Block\Adminhtml\Content\AutoTest"
                   name="adminhtml.packlink.autoTest"
                   template="Packlink_PacklinkPro::autoTest.phtml"
                   cacheable="false"
            />
        </referenceContainer>
    </body>
</page>
